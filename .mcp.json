{
	"mcpServers": {
		"taskmaster-ai": {
			"type": "stdio",
			"command": "pnpx",
			"args": [
				"-y",
				"--package=task-master-ai",
				"task-master-ai"
			]
		},
		"neo4j-lapack": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"mcp-neo4j-cypher@0.3.0"
			],
			"env": {
				"NEO4J_URI": "bolt://localhost:7687",
				"NEO4J_USERNAME": "",
				"NEO4J_PASSWORD": "",
				"NEO4J_DATABASE": "neo4j"
			}
		},
		"git": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"mcp-server-git"
			]
		},
		"github": {
			"type": "stdio",
			"command": "pnpx",
			"args": [
				"@modelcontextprotocol/server-github"
			]
		},
		"nixos": {
			"type": "stdio",
			"command": "nix",
			"args": [
				"run",
				"github:utensils/mcp-nixos",
				"--"
			]
		}
	}
}
