{
	"mcpServers": {
		"claude-flow": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"claude-flow@alpha",
				"mcp",
				"start"
			]
		},
		"ruv-swarm": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"ruv-swarm@latest",
				"mcp",
				"start"
			]
		},
		"taskmaster-ai": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"--package=task-master-ai",
				"task-master-ai"
			]
		}
	}
}
